<script>
	// 카운트가 가능한 경우
	for(i=1; i<=10; i++){
		console.log(i);
	}
	
	// 몇번 수행할지 모르는 경우 한번도 수행이 안될 수 있음
	i=1;
	while(i<=10){
		console.log(i);
		i++;
	}
	
	for(i=1; i<=10; i++){
		document.write("이서연");
	}
</script>
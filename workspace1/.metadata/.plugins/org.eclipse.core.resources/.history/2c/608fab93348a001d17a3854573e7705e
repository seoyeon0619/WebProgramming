<script>
// 함수정의 
function hello(name)
{
	console.log("안녕하세요" + name + "님")
}
// 함수호출
hello("홍길동");
console.log(hello("장길산")); 
	// 에러가 발생하지 않음
	// None 출력 
	
// 1~N까지 출력하는 함수 만들기
function displayNumber(limit)
	{
		for(i=1; i<=limit; i++)
			{
			console.log("i=", i);
			}
	}
displayNumber(10);
displayNumber(5);

</script>